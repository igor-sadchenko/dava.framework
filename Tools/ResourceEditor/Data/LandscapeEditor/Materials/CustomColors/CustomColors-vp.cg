VPROG_IN_BEGIN
    VPROG_IN_POSITION
    VPROG_IN_TEXCOORD
    VPROG_IN_COLOR0
VPROG_IN_END

VPROG_OUT_BEGIN
    VPROG_OUT_POSITION
    VPROG_OUT_TEXCOORD0(uv,2)
    VPROG_OUT_COLOR0(color,4)
VPROG_OUT_END

property float4x4 worldViewProjMatrix : dynamic,instance : ;

VPROG_BEGIN

    VP_OUT_POSITION = mul( float4(VP_IN_POSITION.xyz,1.0), worldViewProjMatrix );
    VP_OUT(uv) = VP_IN_TEXCOORD;
    VP_OUT(color) =  VP_IN_COLOR0;

VPROG_END
